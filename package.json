    "leaflet": "^1.3.4",
    "memory-cache": "^0.2.0",
    "moment": "^2.22.2",
    "node-geocoder": "^3.22.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-leaflet": "^2.0.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  }
}
